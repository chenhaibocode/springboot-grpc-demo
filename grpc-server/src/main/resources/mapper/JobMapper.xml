<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.chenhaibo.dao.JobMapper">
    <resultMap id="job" type="com.chenhaibo.model.Job"/>

    <select id="getJobById" parameterType="Long" resultMap="job">
        select t.id, t.jobCode from job t where t.id = #{value}
    </select>
</mapper>