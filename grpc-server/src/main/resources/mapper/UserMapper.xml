<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.chenhaibo.dao.UserMapper">
    <resultMap id="user" type="com.chenhaibo.model.User"/>

    <select id="findByUserName" parameterType="String" resultMap="user">
        select t.id, t.name from User t where t.name = #{value}
    </select>
</mapper>